{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "msg",
    "data"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "number",
      "title": "The Code Schema",
      "default": 0.0,
      "examples": [
        -8517645.651734278
      ]
    },
    "msg": {
      "$id": "#/properties/msg",
      "type": "string",
      "title": "The Msg Schema",
      "default": "",
      "examples": [
        "veniam adipisicing"
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "key",
        "qr_url",
        "expire_time"
      ],
      "properties": {
        "key": {
          "$id": "#/properties/data/properties/key",
          "type": "string",
          "title": "The Key Schema",
          "default": "",
          "examples": [
            "qui veniam et"
          ]
        },
        "qr_url": {
          "$id": "#/properties/data/properties/qr_url",
          "type": "string",
          "title": "The Qr_url Schema",
          "default": "",
          "examples": [
            "exercitation pariatur esse"
          ]
        },
        "expire_time": {
          "$id": "#/properties/data/properties/expire_time",
          "type": "number",
          "title": "The Expire_time Schema",
          "default": 0.0,
          "examples": [
            -10892265.680504963
          ]
        }
      }
    }
  }
}