{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "msg",
    "data"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "number",
      "title": "The Code Schema",
      "default": 0.0,
      "examples": [
        73241560.2706717
      ]
    },
    "msg": {
      "$id": "#/properties/msg",
      "type": "string",
      "title": "The Msg Schema",
      "default": "",
      "examples": [
        "laboris"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "broadcastWords"
      ],
      "properties": {
        "broadcastWords": {
          "$id": "#/properties/data/properties/broadcastWords",
          "type": "array",
          "title": "The Broadcastwords Schema",
          "items": {
            "$id": "#/properties/data/properties/broadcastWords/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "magna Lorem",
              "dolor in ut sit"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}