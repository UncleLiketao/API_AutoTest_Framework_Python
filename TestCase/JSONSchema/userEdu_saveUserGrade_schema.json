{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "msg",
    "data"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "number",
      "title": "The Code Schema",
      "default": 0.0,
      "examples": [
        -32961153.51220741
      ]
    },
    "msg": {
      "$id": "#/properties/msg",
      "type": "string",
      "title": "The Msg Schema",
      "default": "",
      "examples": [
        "minim aliquip"
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "toast"
      ],
      "properties": {
        "toast": {
          "$id": "#/properties/data/properties/toast",
          "type": "string",
          "title": "The Toast Schema",
          "default": "",
          "examples": [
            "Lorem"
          ]
        }
      }
    }
  }
}